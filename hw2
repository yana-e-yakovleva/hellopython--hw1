#task1

f = input(int())
a = 0
b = 0
for i in f:
        if int(i) == 1:
            a += 1
        else:
            b += 1

print(a if a < b else b)


#task2

a, b = map(int, input().split())
c = 0
for i in range(a + b):
    if c:
        break
    for j in range(a + b):
        if i + j == a and i * j == b:
            c = 1
            print(*sorted([i, j]))
            break
            
 #task3
 
 n=int(input())
p=1
while p<=n:
    print(p,end=' ')
    p=p*2
